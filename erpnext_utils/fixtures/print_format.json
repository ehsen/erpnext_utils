  "html": "<div class=\"do-print-wrap\">\n  <div class=\"half-a4\" style=\"width: 100%; height: 148mm; padding: 15px 20px 80px 20px; font-family: Arial, sans-serif; font-size: 12px; box-sizing: border-box; position: relative;\">\n\n    <!-- Header Section -->\n    <div style=\"text-align: center; margin-bottom: 20px;\">\n      <h2 style=\"margin: 0; font-size: 24px; font-weight: bold;\">{{ doc.company or '' }}</h2>\n      \n      <!-- Company Address -->\n      {% set company_address = frappe.get_list('Address', filters={'is_your_company_address': 1, 'address_type': 'Billing'}, fields=['address_line1', 'address_line2', 'city', 'state', 'country', 'pincode', 'phone'], limit=1) %}\n      {% if company_address and company_address|length > 0 %}\n        <div style=\"margin-top: 5px; font-size: 11px;\">\n          {{ company_address[0].address_line1 or '' }}{% if company_address[0].address_line2 %}, {{ company_address[0].address_line2 }}{% endif %}, {{ company_address[0].city or '' }}{% if company_address[0].state %}, {{ company_address[0].state }}{% endif %}, {{ company_address[0].country or '' }}{% if company_address[0].pincode %} - {{ company_address[0].pincode }}{% endif %}{% if company_address[0].phone %}, Phone: {{ company_address[0].phone }}{% endif %}\n        </div>\n      {% endif %}\n    </div>\n\n    <!-- Title -->\n    <div style=\"text-align: center; margin-bottom: 20px;\">\n      <h3 style=\"margin: 0; font-size: 18px; font-weight: bold; text-decoration: underline;\">GATE ENTRY - {{ doc.gate_entry_type or '' }}</h3>\n    </div>\n\n    <!-- Document Details -->\n    <div style=\"margin-bottom: 15px;\">\n      <table style=\"width: 100%; border-collapse: collapse;\">\n        <tr>\n          <td style=\"width: 15%; border: none; padding: 2px;\"><strong>Gate Entry No:</strong></td>\n          <td style=\"width: 35%; border: none; padding: 2px;\">{{ doc.name or '' }}</td>\n          <td style=\"width: 15%; border: none; padding: 2px;\"><strong>Date:</strong></td>\n          <td style=\"width: 35%; border: none; padding: 2px;\">{{ frappe.utils.format_date(doc.gate_entry_date, \"dd-MM-yyyy\") if doc.gate_entry_date else '' }}</td>\n        </tr>\n        <tr>\n          <td style=\"border: none; padding: 2px;\"><strong>Type:</strong></td>\n          <td style=\"border: none; padding: 2px;\">{{ doc.gate_entry_type or '' }}</td>\n          <td style=\"border: none; padding: 2px;\"><strong>Status:</strong></td>\n          <td style=\"border: none; padding: 2px;\">{{ doc.status or '' }}</td>\n        </tr>\n        {% if doc.gate_entry_type == 'Inward' %}\n        <tr>\n          <td style=\"border: none; padding: 2px;\"><strong>Supplier:</strong></td>\n          <td style=\"border: none; padding: 2px;\">{{ doc.supplier_name or '' }}</td>\n          <td style=\"border: none; padding: 2px;\"><strong>Vehicle No:</strong></td>\n          <td style=\"border: none; padding: 2px;\">{{ doc.vehicle_number or '' }}</td>\n        </tr>\n        {% else %}\n        <tr>\n          <td style=\"border: none; padding: 2px;\"><strong>Customer:</strong></td>\n          <td style=\"border: none; padding: 2px;\">{{ doc.customer_name or '' }}</td>\n          <td style=\"border: none; padding: 2px;\"><strong>Vehicle No:</strong></td>\n          <td style=\"border: none; padding: 2px;\">{{ doc.vehicle_number or '' }}</td>\n        </tr>\n        {% endif %}\n        <tr>\n          <td style=\"border: none; padding: 2px;\"><strong>Driver:</strong></td>\n          <td style=\"border: none; padding: 2px;\">{{ doc.driver_name or '' }}</td>\n          <td style=\"border: none; padding: 2px;\"><strong>Contact:</strong></td>\n          <td style=\"border: none; padding: 2px;\">{{ doc.contact_number or '' }}</td>\n        </tr>\n        <tr>\n          <td style=\"border: none; padding: 2px;\"><strong>Purpose:</strong></td>\n          <td colspan=\"3\" style=\"border: none; padding: 2px;\">{{ doc.purpose or '' }}</td>\n        </tr>\n      </table>\n    </div>\n\n    <!-- Items Table -->\n    <div style=\"margin-bottom: 20px;\">\n      <table style=\"width: 100%; border-collapse: collapse; font-size: 11px;\">\n        <thead>\n          <tr>\n            <th style=\"text-align: center; width: 5%;\">#</th>\n            <th style=\"text-align: left; width: 15%;\">Item Code</th>\n            <th style=\"text-align: left; width: 35%;\">Item Name</th>\n            <th style=\"text-align: center; width: 10%;\">UOM</th>\n            <th style=\"text-align: right; width: 15%;\">QTY</th>\n            <th style=\"text-align: right; width: 10%;\">Rate</th>\n            <th style=\"text-align: right; width: 10%;\">Amount</th>\n          </tr>\n        </thead>\n        <tbody>\n          {% for item in doc.items %}\n          <tr>\n            <td style=\"text-align: center;\">{{ loop.index }}</td>\n            <td style=\"padding-left: 4px;\">{{ item.item_code or '' }}</td>\n            <td style=\"padding-left: 4px;\">{{ item.item_name or '' }}</td>\n            <td style=\"text-align: center;\">{{ item.uom or '' }}</td>\n            <td style=\"text-align: right; padding-right: 4px;\">{{ \"{:,.2f}\".format(item.qty or 0) }}</td>\n            <td style=\"text-align: right; padding-right: 4px;\">{{ \"{:,.2f}\".format(item.rate or 0) }}</td>\n            <td style=\"text-align: right; padding-right: 4px;\">{{ \"{:,.2f}\".format(item.amount or 0) }}</td>\n          </tr>\n          {% endfor %}\n        </tbody>\n        <tfoot>\n          <tr style=\"font-weight: bold; background-color: #f0f0f0;\">\n            <td colspan=\"4\" style=\"text-align: right; padding-right: 4px;\">Total:</td>\n            <td style=\"text-align: right; padding-right: 4px;\">{{ \"{:,.2f}\".format(doc.total_qty or 0) }}</td>\n            <td style=\"text-align: right; padding-right: 4px;\">-</td>\n            <td style=\"text-align: right; padding-right: 4px;\">{{ \"{:,.2f}\".format(doc.total_amount or 0) }}</td>\n          </tr>\n        </tfoot>\n      </table>\n    </div>\n\n    <!-- Remarks -->\n    {% if doc.remarks %}\n    <div style=\"margin-bottom: 20px; font-size: 11px;\">\n      <strong>Remarks:</strong> {{ doc.remarks }}\n    </div>\n    {% endif %}\n\n    <!-- Signature Section -->\n    <div class=\"signature-section\">\n      <div class=\"signature-row\">\n        <div class=\"signature-item\">\n          <strong>Gate Keeper</strong>\n          <div class=\"signature-line\"></div>\n        </div>\n        <div class=\"signature-item\">\n          <strong>Security Guard</strong>\n          <div class=\"signature-line\"></div>\n        </div>\n        <div class=\"signature-item\">\n          <strong>Driver</strong>\n          <div class=\"signature-line\"></div>\n        </div>\n        <div class=\"signature-item\">\n          <strong>Supervisor</strong>\n          <div class=\"signature-line\"></div>\n        </div>\n        <div class=\"signature-item\">\n          <strong>Manager</strong>\n          <div class=\"signature-line\"></div>\n        </div>\n      </div>\n    </div>\n\n  </div>\n</div>\n",
