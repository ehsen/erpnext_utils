  "html": "<div style=\"width: 100%; padding: 20px; font-family: Arial, sans-serif; font-size: 12px; box-sizing: border-box;\">\n\n  <!-- Header Section -->\n  <div style=\"display: flex; justify-content: space-between; align-items: flex-start; margin-bottom: 20px;\">\n    <!-- Company Details -->\n    <div style=\"width: 50%; text-align: left;\">\n      {% set company_address = frappe.get_list('Address', filters={'is_your_company_address': 1, 'address_type': 'Billing'}, fields=['address_line1', 'city', 'state', 'country', 'phone'], limit=1) %}\n      <strong style=\"font-size: 16px;\">{{ doc.company or '' }}</strong><br>\n      {% if company_address and company_address|length > 0 %}\n        {{ company_address[0].address_line1 or '' }}<br>\n        {{ company_address[0].city or '' }}{% if company_address[0].state %}, {{ company_address[0].state }}{% endif %}<br>\n        {{ company_address[0].country or '' }}<br>\n        {% if company_address[0].phone %}\n          Phone: {{ company_address[0].phone }}<br>\n        {% endif %}\n      {% endif %}\n    </div>\n    \n    <!-- Title -->\n    <div style=\"width: 50%; text-align: right;\">\n      <h2 style=\"margin: 0; font-size: 24px; font-weight: bold; text-decoration: underline;\">STOCK ENTRY</h2>\n      {% if doc.stock_entry_type %}\n      <p style=\"margin: 5px 0 0 0; font-size: 14px; font-weight: bold;\">{{ doc.stock_entry_type }}</p>\n      {% endif %}\n    </div>\n  </div>\n\n  <!-- Stock Entry Details -->\n  <div style=\"margin-bottom: 20px; border: 2px solid #000; padding: 10px;\">\n    <table style=\"width: 100%; border-collapse: collapse;\">\n      <tr>\n        <td style=\"width: 25%; border: none; padding: 4px; font-size: 13px;\"><strong>Stock Entry No:</strong></td>\n        <td style=\"width: 25%; border: none; padding: 4px; font-size: 13px;\">{{ doc.name or '' }}</td>\n        <td style=\"width: 25%; border: none; padding: 4px; font-size: 13px;\"><strong>Purpose:</strong></td>\n        <td style=\"width: 25%; border: none; padding: 4px; font-size: 13px;\">{{ doc.purpose or '' }}</td>\n      </tr>\n      <tr>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\"><strong>Posting Date:</strong></td>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\">{{ frappe.utils.format_date(doc.posting_date, \"dd-MM-yyyy\") if doc.posting_date else '' }}</td>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\"><strong>Posting Time:</strong></td>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\">{{ doc.posting_time or '' }}</td>\n      </tr>\n      <tr>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\"><strong>From Warehouse:</strong></td>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\">{{ doc.from_warehouse or 'N/A' }}</td>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\"><strong>To Warehouse:</strong></td>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\">{{ doc.to_warehouse or 'N/A' }}</td>\n      </tr>\n      {% if doc.work_order %}\n      <tr>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\"><strong>Work Order:</strong></td>\n        <td colspan=\"3\" style=\"border: none; padding: 4px; font-size: 13px;\">{{ doc.work_order or '' }}</td>\n      </tr>\n      {% endif %}\n      {% if doc.purchase_order %}\n      <tr>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\"><strong>Purchase Order:</strong></td>\n        <td colspan=\"3\" style=\"border: none; padding: 4px; font-size: 13px;\">{{ doc.purchase_order or '' }}</td>\n      </tr>\n      {% endif %}\n      {% if doc.delivery_note_no %}\n      <tr>\n        <td style=\"border: none; padding: 4px; font-size: 13px;\"><strong>Delivery Note:</strong></td>\n        <td colspan=\"3\" style=\"border: none; padding: 4px; font-size: 13px;\">{{ doc.delivery_note_no or '' }}</td>\n      </tr>\n      {% endif %}\n    </table>\n  </div>\n\n  <!-- Items Table -->\n  <div style=\"margin-bottom: 20px;\">\n    <h3 style=\"margin: 10px 0; font-size: 16px; font-weight: bold;\">Items</h3>\n    <table style=\"width: 100%; border-collapse: collapse; font-size: 11px;\">\n      <thead>\n        <tr>\n          <th style=\"text-align: center; width: 4%;\">#</th>\n          <th style=\"text-align: left; width: 12%;\">Item Code</th>\n          <th style=\"text-align: left; width: 20%;\">Item Name</th>\n          <th style=\"text-align: center; width: 12%;\">Source Warehouse</th>\n          <th style=\"text-align: center; width: 12%;\">Target Warehouse</th>\n          <th style=\"text-align: right; width: 8%;\">Qty</th>\n          <th style=\"text-align: center; width: 6%;\">UOM</th>\n          <th style=\"text-align: right; width: 10%;\">Rate</th>\n          <th style=\"text-align: right; width: 12%;\">Amount</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for item in doc.items %}\n        <tr>\n          <td style=\"text-align: center; vertical-align: top;\">{{ loop.index }}</td>\n          <td style=\"padding-left: 4px; vertical-align: top;\">{{ item.item_code or '' }}</td>\n          <td style=\"padding-left: 4px; vertical-align: top;\">{{ item.item_name or '' }}</td>\n          <td style=\"text-align: center; vertical-align: top;\">{{ item.s_warehouse or '-' }}</td>\n          <td style=\"text-align: center; vertical-align: top;\">{{ item.t_warehouse or '-' }}</td>\n          <td style=\"text-align: right; padding-right: 4px; vertical-align: top;\">{{ \"{:,.2f}\".format(item.qty or 0) }}</td>\n          <td style=\"text-align: center; vertical-align: top;\">{{ item.uom or '' }}</td>\n          <td style=\"text-align: right; padding-right: 4px; vertical-align: top;\">{{ \"{:,.2f}\".format(item.basic_rate or 0) }}</td>\n          <td style=\"text-align: right; padding-right: 4px; vertical-align: top;\">{{ \"{:,.2f}\".format(item.amount or 0) }}</td>\n        </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  </div>\n\n  <!-- Value Summary -->\n  <div style=\"margin-bottom: 20px; border: 1px solid #000; padding: 10px;\">\n    <h3 style=\"margin: 0 0 10px 0; font-size: 14px; font-weight: bold;\">Value Summary</h3>\n    <table style=\"width: 100%; border-collapse: collapse;\">\n      <tr>\n        <td style=\"width: 33%; border: none; padding: 4px;\"><strong>Total Outgoing Value:</strong></td>\n        <td style=\"width: 33%; border: none; padding: 4px;\">{{ \"{:,.2f}\".format(doc.total_outgoing_value or 0) }}</td>\n        <td style=\"width: 34%; border: none; padding: 4px;\"></td>\n      </tr>\n      <tr>\n        <td style=\"border: none; padding: 4px;\"><strong>Total Incoming Value:</strong></td>\n        <td style=\"border: none; padding: 4px;\">{{ \"{:,.2f}\".format(doc.total_incoming_value or 0) }}</td>\n        <td style=\"border: none; padding: 4px;\"></td>\n      </tr>\n      <tr>\n        <td style=\"border: none; padding: 4px;\"><strong>Value Difference:</strong></td>\n        <td style=\"border: none; padding: 4px;\">{{ \"{:,.2f}\".format(doc.value_difference or 0) }}</td>\n        <td style=\"border: none; padding: 4px;\"></td>\n      </tr>\n      {% if doc.total_additional_costs and doc.total_additional_costs > 0 %}\n      <tr>\n        <td style=\"border: none; padding: 4px;\"><strong>Total Additional Costs:</strong></td>\n        <td style=\"border: none; padding: 4px;\">{{ \"{:,.2f}\".format(doc.total_additional_costs or 0) }}</td>\n        <td style=\"border: none; padding: 4px;\"></td>\n      </tr>\n      {% endif %}\n      <tr style=\"border-top: 2px solid #000;\">\n        <td style=\"border: none; padding: 8px 4px; font-size: 14px;\"><strong>Total Amount:</strong></td>\n        <td style=\"border: none; padding: 8px 4px; font-size: 14px;\"><strong>{{ \"{:,.2f}\".format(doc.total_amount or 0) }}</strong></td>\n        <td style=\"border: none; padding: 8px 4px;\"></td>\n      </tr>\n    </table>\n  </div>\n\n  <!-- Additional Costs Table -->\n  {% if doc.additional_costs %}\n  <div style=\"margin-bottom: 20px;\">\n    <h3 style=\"margin: 10px 0; font-size: 14px; font-weight: bold;\">Additional Costs</h3>\n    <table style=\"width: 100%; border-collapse: collapse; font-size: 12px;\">\n      <thead>\n        <tr>\n          <th style=\"text-align: center; width: 10%;\">#</th>\n          <th style=\"text-align: left; width: 50%;\">Description</th>\n          <th style=\"text-align: right; width: 40%;\">Amount</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for cost in doc.additional_costs %}\n        <tr>\n          <td style=\"text-align: center;\">{{ loop.index }}</td>\n          <td style=\"padding-left: 4px;\">{{ cost.description or '' }}</td>\n          <td style=\"text-align: right; padding-right: 4px;\">{{ \"{:,.2f}\".format(cost.amount or 0) }}</td>\n        </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  </div>\n  {% endif %}\n\n  <!-- Remarks -->\n  {% if doc.remarks %}\n  <div style=\"margin-bottom: 20px;\">\n    <strong>Remarks:</strong><br>\n    <div style=\"padding: 10px; border: 1px solid #ddd; background-color: #f9f9f9;\">\n      {{ doc.remarks }}\n    </div>\n  </div>\n  {% endif %}\n\n  <!-- Signature Section -->\n  <div style=\"margin-top: 60px; display: flex; justify-content: space-between;\">\n    <div style=\"width: 30%; text-align: center;\">\n      <div style=\"border-top: 2px solid #000; padding-top: 5px;\">\n        <strong>Prepared By</strong>\n      </div>\n    </div>\n    <div style=\"width: 30%; text-align: center;\">\n      <div style=\"border-top: 2px solid #000; padding-top: 5px;\">\n        <strong>Verified By</strong>\n      </div>\n    </div>\n    <div style=\"width: 30%; text-align: center;\">\n      <div style=\"border-top: 2px solid #000; padding-top: 5px;\">\n        <strong>Approved By</strong>\n      </div>\n    </div>\n  </div>\n\n  <!-- Footer Note -->\n  <div style=\"margin-top: 30px; text-align: center; font-size: 10px; color: #666; border-top: 1px solid #ccc; padding-top: 10px;\">\n    This is a computer-generated document. For any queries, please contact the warehouse department.<br>\n    {{ doc.company or '' }} | Stock Entry: {{ doc.name or '' }}\n  </div>\n\n</div>\n",
